{
    "useFolders": true,
    "stage": "dev",
    "cacheDirectory": "_cache",
    "assetDirectories": {
        "js": {
            "main": "javascripts",
            "fallbacks": []
        },
        "coffee": {
            "main": "coffeescript",
            "fallbacks": []
        },
        "less": {
            "main": "styles",
            "fallbacks": []
        },
        "sass": {
            "main": "styles",
            "fallbacks": []
        },
        "scss": {
            "main": "styles",
            "fallbacks": []
        },
        "css": {
            "main": "styles",
            "fallbacks": []
        },
        "img": {
            "main": "img",
            "fallbacks": []
        },
        "font": {
            "main": "fonts",
            "fallbacks": []
        }
    },
    "mimeTypes": {
        "jpg": "image/jpg",
        "png": "image/png",
        "gif": "image/gif",
        "eot": "application/vnd.ms-fontobject",
        "ttf": "application/octet-stream",
        "svg": "image/svg+xml",
        "woff": "application/x-woff"
    }
}