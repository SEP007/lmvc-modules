        "handlers": {
            "php": {
                "namespace": "\\Scandio\\lmvc\\modules\\rendering\\handlers\\PhpHandler",
                "extension": "html"
            },
            "smarty": {
                "namespace": "\\Scandio\\lmvc\\modules\\rendering\\handlers\\SmartyHandler",
                "extension": "smarty",
                "directories": {
                    "template": "smarty/templates",
                    "compile": "smarty/compile",
                    "cache": "smarty/_cache",
                    "config": "smarty/config"
